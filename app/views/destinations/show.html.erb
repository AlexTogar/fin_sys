<div class="page-header">
  <%= link_to destinations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_destination_path(@destination), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show destination</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @destination.name %></dd>

  <dt>Sum:</dt>
  <dd><%= @destination.sum %></dd>

  <dt>End date:</dt>
  <dd><%= @destination.end_date %></dd>

  <dt>Portion sum:</dt>
  <dd><%= @destination.portion_sum %></dd>

  <dt>Description:</dt>
  <dd><%= @destination.description %></dd>

  <dt>Deleted:</dt>
  <dd><%= @destination.deleted %></dd>

</dl>
