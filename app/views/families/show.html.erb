<div class="page-header">
  <%= link_to families_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_family_path(@family), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show family</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @family.name %></dd>

  <dt>Connect:</dt>
  <dd><%= @family.connect %></dd>

  <dt>Deleted:</dt>
  <dd><%= @family.deleted %></dd>

  <dt>User:</dt>
  <dd><%= @family.user %></dd>

</dl>
